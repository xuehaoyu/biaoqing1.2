<!--pages/morenew/morenew.wxml-->
<view class='new-body'>
  <view class='new-list'>
      <view class='new-item' wx:for="{{newList}}" wx:key="{{item.GroupID}}">
        <view class='new-img'>
          <image src='{{item.ImageUrl}}' mode='aspectFill'></image>
        </view>
        <view class='new-info'>
          <view class='info-title'>{{item.GroupName}}</view>
          <view class='info-dec'>{{item.Tags}}</view>
        </view>
      </view>
  </view>
</view>

